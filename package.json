{
  "name": "nvc",
  "version": "0.0.0-semantic-release",
  "description": "module nvc",
  "bin": {
    "nvc": "cli.js"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "prettier-standard '**/*.{js,md}'",
    "test": "jest --coverage",
    "precommit": "npm run lint && npm test",
    "commitmsg": "validate-commit-msg"
  },
  "keywords": [
    "npm",
    "version",
    "cli",
    "package.json"
  ],
  "author": "herrmannplatz <rene@herrmannplatz.de>",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "execa": "^0.10.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "prettier-standard": "^8.0.1",
    "standard": "^11.0.1",
    "validate-commit-msg": "^2.14.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/herrmannplatz/nvc.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
